@using System.Web.Optimization;
@model  Claro.SIACU.Web.WebApplication.Areas.Management.Models.InstantModel
@{
    Layout = null;
}
<div id="contenedorVisualizeCall">



    <div class="row">
        <div class="col-md-12">
            <div class="form-inline">

                <label class="text-primary">Fecha:</label>

                <div class="input-group date" data-provide="datepicker" data-date-format="dd/mm/yyyy" lang="es-pe">
                    <input type="text" class="form-control" value="@DateTime.Now.ToString("dd/MM/yyyy")" id="txtDateInstant" readonly>
                    <div class="input-group-addon">
                        <span class="glyphicon glyphicon-th"></span>
                    </div>
                </div>
                &nbsp;&nbsp;
                <button id="btnSearchInstant" type="button" class="btn claro-btn-info btn-sm">Buscar</button>

            </div>
            <div class="form-inline">

                <label class="text-primary">Hora:</label>
                <input type="text" id="txthours" class="input-sm form-control" name="start" value="" size="1" />
                <label class="text-primary">:</label>
                <input type="text" id="txtminutes" class="input-sm form-control" name="start" value="" size="1" />
                <select class="input-sm form-control" id="cboType">
                    <option value="-1">Seleccionar</option>
                    <option value="AM">AM</option>
                    <option value="PM">PM</option>
                </select>
                &nbsp;&nbsp;
                <label class="text-primary">formato de horas => hh:mm AM/PM</label>

            </div>
            <div class="form-inline">

                &nbsp;&nbsp;
                <button id="btnNewInstant" type="button" class="btn claro-btn-info btn-sm">Nuevo</button>
                &nbsp;&nbsp;
                <button id="btnEditInstant" type="button" class="btn claro-btn-info btn-sm">Modificar</button>
                &nbsp;&nbsp;
                <button id="btnDeleteInstant" type="button" class="btn claro-btn-info btn-sm">Eliminar</button>

            </div>

        </div>
    </div>


    <div class="table-responsive">
        <div id="divDetailVisualizeCall" style="overflow-y: scroll; ">

        </div>
    </div>
   
        <label class="text-primary">Leyenda:</label>
        <ul id="divDashboardTabs" class="nav nav-tabs" role="tablist" data-tabs="tabs">
            <li id="PostImportanceHight" style="border-bottom:0" class="active">

                <div> <div style="background-color: red; width: 14px;height:12px; float: left;margin-top:3px;margin-right:3px;">&nbsp;</div>&nbsp;&nbsp;<div style="float:left;">Importancia Alta</div></div>

            </li>

            <li id="PostImportanceMediumHight" class="active">

                <div> <div style="background-color: green; width: 14px; height: 12px; float: left; margin-top: 3px; margin-right: 3px; ">&nbsp;</div>&nbsp;&nbsp;<div style="float:left;">Importancia Media Alta</div></div>

            </li>
            <li id="PostImortanceMedium" class="active">

                <div> <div style="background-color: orange; width: 14px; height: 12px; float: left; margin-top: 3px; margin-right: 3px; ">&nbsp;</div>&nbsp;&nbsp;<div style="float:left;">Importancia Media</div></div>

            </li>
            <li id="PostDefault" class="active">

                <div> <div style="background-color: #3e8dd0; width: 14px; height: 12px; float: left; margin-top: 3px; margin-right: 3px; ">&nbsp;</div>&nbsp;&nbsp;<div style="float:left;">Default</div></div>

            </li>
        </ul>

    
</div>




@Scripts.Render("~/bundles/Content/Lib/BloqueoF12")
@Scripts.Render("~/bundles/Management/VisualizeInstant/VisualizeInstant")