@using System.Web.Optimization;
@{
    Layout = null;
}
    <div id="ContentMassiveInstant">
        <div class="row">
            <div class="col-md-11">
                <label class="text-primary">Descipción:</label>
                <textarea class="form-control" rows="2" id="txtDescription" style="resize:none"></textarea>
            </div>
        </div>
        <div class="row">
            <div align="center">
                <div class="col-md-7" style="float:none; margin:0 auto">
                    <input type="file" id="FileText" class="input-sm form-control" name="" value="FileText" size="1" />
                </div>
            </div>
        </div>
        <div class="row">
            <div id="modal-content" align="center">
                <div class="col-md-7" style="float:none; margin:0 auto">
                    @*Instantanea Masiva**@
                    <div id="InstMasiveFrame" class="panel panel-info">
                        <div class="panel-heading claro-modal-contenido-titulo-panel">
                            <h4 class="panel-title claro-modal-contenido-titulo-texto">
                                <a data-toggle="collapse" href="#"> Relación de Líneas</a>
                            </h4>
                        </div>
                        <div id="" class="panel-collapse collapse in table-responsive ">
                            <table class="table  table-striped table-hover table-responsive claro-modal-tabla" id="tblInsMassCorrect">
                                <thead class="claro-bg-primary">
                                    <tr>
                                        <th class="col-sm-1">Telefonos</th>
                                    </tr>
                                </thead>
                                <tbody style=" text-align: center;">
                                </tbody>
                            </table>
                        </div>

                        <label id="lblCountLine" class="text-primary" style="float:left"></label>
                    </div>
                    @*******@

                    @*Realizar Petición Carga*@
                    <div id="MakeChargePetitionFrame">
                        <label id="FileNameInst" class="text-primary"></label> <span id='ClearFile' class='label label-danger' title='Borrar Archivo' style='cursor:pointer; margin-left: 3px'>X</span>
                    </div>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-md-12">
                <div class="form-inline">
                    <label class="text-primary">Vigente del :</label>
                    <div class="input-daterange input-group" id="containerDateRange">
                        <input type="text" id="txtDateStart" class="input-sm form-control" name="start" />
                        <label class="input-group-addon btn" for="txtDateStart">
                            <span class="glyphicon glyphicon-calendar"></span>
                        </label>
                    </div>
                    <input type="text" id="txthoursVig" class="input-sm form-control" name="start" size="1" maxlength="2" />
                    <label class="text-primary">:</label>
                    <input type="text" id="txtminutesVig" class="input-sm form-control" name="start" size="1" maxlength="2" />
                    <select class="input-sm form-control" id="cboTypeVig">
                        <option value="-1">Seleccionar</option>
                        <option value="AM">AM</option>
                        <option value="PM">PM</option>
                    </select>
                    &nbsp;&nbsp;
                    <label class="text-primary">formato de horas => hh:mm AM/PM</label>
                </div>

                &nbsp;&nbsp;

                <div class="form-inline">
                    <label class="text-primary">Caduca &nbsp;&nbsp;el :</label>
                    <div class="input-daterange input-group" id="containerDateRange">
                        <input type="text" id="txtDateEnd" class="input-sm form-control" name="end" />
                        <label class="input-group-addon btn" for="txtDateEnd">
                            <span class="glyphicon glyphicon-calendar"></span>
                        </label>
                    </div>
                    <input type="text" id="txthoursCad" class="input-sm form-control" name="start" value="" size="1" maxlength="2" />
                    <label class="text-primary">:</label>
                    <input type="text" id="txtminutesCad" class="input-sm form-control" name="start" value="" size="1" maxlength="2" />
                    <select class="input-sm form-control" id="cboTypeCad">
                        <option value="-1">Seleccionar</option>
                        <option value="AM">AM</option>
                        <option value="PM">PM</option>
                    </select>
                    &nbsp;&nbsp;
                    <label class="text-primary">formato de horas => hh:mm AM/PM</label>
                </div>
            </div>
        </div>
    </div>
&nbsp;&nbsp;
@Scripts.Render("~/bundles/Content/Lib/BloqueoF12")
   @Scripts.Render("~/bundles/Management/MassiveInstant/MassiveInstant")