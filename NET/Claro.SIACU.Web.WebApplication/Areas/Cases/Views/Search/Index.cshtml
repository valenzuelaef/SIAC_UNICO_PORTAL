@using System.Web.Optimization;
<div id="container" class="tab-content">
    <div id="searchCases" class="panel panel-info searchCases">
        <div class="panel-heading claro-modal-contenido-titulo-panel">
            <h4 class="panel-title claro-modal-contenido-titulo-texto">
                Consulta de Casos
            </h4>
        </div>
        <div class="row">
            <div class="col-md-12">
                <input type="radio" name="searchCasefilter" id="searchCaseByCaseId" checked="checked" />
                <label for="searchCaseByCaseId">Nro. Caso</label>
                <input type="radio" name="searchCaseFilter" id="searchCaseByReportId" />
                <label for="searchCaseByReportId">Nro. Reporte</label>
                <input type="radio" name="searchCaseFilter" id="searchCaseByComplaintId" />
                <label for="searchCaseByComplaintId">Nro. Reclamo</label>
                <input type="text" name="filter" id="searchCaseText" />
                <a href="#" id="searchCaseStart" class="btn claro-btn-info btn-sm">Buscar</a>
                <a href="#" id="searchCaseClear" class="btn claro-btn-info btn-sm">Limpiar</a>
            </div>
        </div>
        <div id="searchCaseResults" class="row">

        </div>
    </div>
</div>
@Scripts.Render("~/bundles/Content/Lib/BloqueoF12")
@Scripts.Render("~/bundles/Cases/Search/Search")