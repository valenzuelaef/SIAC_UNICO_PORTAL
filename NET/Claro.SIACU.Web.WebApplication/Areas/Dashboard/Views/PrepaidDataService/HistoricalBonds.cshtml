@using System.Web.Optimization
@model Claro.SIACU.Web.WebApplication.Areas.Dashboard.Models.PrepaidDataService.HistoricalBondsModel
@{
    Layout = null;
}

<div id="ContentHistoricalBonds">
    <div class="modal-body">
        <div id="modal-content">
            <div class="panel panel-info">


                <div class="panel-body claro-modal-contenido-texto">

                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-inline">
                                <label class="control-label">Desde:</label>
                                <div class="input-daterange input-group" id="containerDateRange">
                                    <input type="text" id="txtDateStartHB" class="input-sm form-control" name="txtDateStartHB" placeholder="Fecha inicial" readonly />
                                    <label class="input-group-addon btn" for="txtDateStartHB">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </label>
                                    <span class="input-group-addon"><label class="control-label">A:</label></span>
                                    <input type="text" id="txtDateEndHB" class="input-sm form-control" name="txtDateEndHB" placeholder="Fecha final" readonly />
                                    <label class="input-group-addon btn" for="txtDateEndHB">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </label>
                                </div>
                                <button id="btnVisualize" type="button" class="btn claro-btn-info btn-sm">Visualizar</button>
                            </div>
                        </div>
                    </div>

                    <table class="table  table-striped table-hover table-responsive claro-modal-tabla table-bordered" id="tblHistoricalBonds">
                        <thead class="claro-bg-primary">
                            <tr>
                                <th>Id_Transacción</th>
                                <th>Transacción</th>
                                <th>Promoción</th>
                                <th>Fecha Registro</th>
                                <th>Aplicativo</th>
                            </tr>
                        </thead>
                    </table>

                </div>

            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/bundles/Content/Lib/BloqueoF12")
@Scripts.Render("~/bundles/Dashboard/PrepaidDataService/HistoricalBonds")