@using System.Web.Optimization;
@model  Claro.SIACU.Web.WebApplication.Areas.Dashboard.Models.PrepaidDataService.PinkPukModel

<div id="containerPinPukSearch">

    <table id="tblPinPukSearch" class="table  table-striped table-hover table-responsive claro-modal-tabla table-bordered">
        <thead class="claro-bg-primary">
            <tr>
                <th>ICC_ID</th>
                <th>IMSI</th>
                <th>MSISDN</th>
                <th>PIN1</th>
                <th>PUK1</th>
                <th>PIN2</th>
                <th>PUK2</th>
            </tr>
        </thead>
        <tbody>
            @if (Model.listPinpukData != null)
            {
                foreach (var item in Model.listPinpukData)
                {
                    <tr>
                        <td>@item.ICC_ID</td>
                        <td>@item.IMSI</td>
                        <td>@item.MSISDN</td>
                        <td>@item.PIN1</td>
                        <td>@item.PUK1</td>
                        <td>@item.PIN2</td>
                        <td>@item.PUK2</td>
                    </tr>
                }
            }
        </tbody>
    </table>

    <div class="row">
        <div class="col-md-12">
            <div class="form-inline">
                <label class="control-label">Observación:</label>
                <br>
                <label>@Model.Answer</label>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/bundles/Content/Lib/BloqueoF12")
@Scripts.Render("~/bundles/Dashboard/PrepaidDataService/PinkpukSearch")
