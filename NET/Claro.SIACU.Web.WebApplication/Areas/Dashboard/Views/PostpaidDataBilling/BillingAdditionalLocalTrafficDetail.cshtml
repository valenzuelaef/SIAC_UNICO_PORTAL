                    </tr>
                </thead>
                <tbody></tbody>
            </table>
            <div id="LoadingPrepaid2"></div>
        </div>
    </div>
</div>
@Scripts.Render("~/bundles/Content/Lib/BloqueoF12")
@Scripts.Render("~/bundles/Dashboard/PostpaidDataBilling/AdditionalLocalTrafficDetail")