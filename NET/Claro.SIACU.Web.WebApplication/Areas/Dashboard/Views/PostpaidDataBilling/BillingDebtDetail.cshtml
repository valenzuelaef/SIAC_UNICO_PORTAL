@using System.Web.Optimization;
@model Claro.SIACU.Web.WebApplication.Areas.Dashboard.Models.PostpaidDataBilling.BillingDebtDetailModel
@{
    Layout = null;
}
<div id="FormDebtDetail">
    <div class="panel panel-info ">
        <div class="table-responsive">
            <table class="table table-condensed">
                <tr>
                    <td class="col-md-1"></td>
                    <td class="col-md-1">
                        <label class="control-label">Número de documento:</label>
                    </td>
                    <td class="col-md-2"><span><label id="DocumentNumber"></label></span></td>
                    <td class="col-md-2"></td>
                    <td class="col-md-1">
                        <label class="control-label">Cuentas encontradas:</label>
                    </td>
                    <td class="col-md-2"><span><label id="DebtQuantity"></label></span> </td>
                </tr>
            </table>
            <table class="table  table-striped table-hover table-responsive claro-modal-tabla" id="tblDebtDetail">
                <thead class="claro-bg-primary">
                    <tr>
                        <th class="col-sm-1">Nro</th>
                        <th class="col-sm-1">Código de Cliente</th>
                        <th class="col-sm-1">Tipo de Cliente</th>
                        <th class="col-sm-1">Nombres</th>
                        <th class="col-sm-1">Apellidos</th>
                        <th class="col-sm-1">Representante</th>
                        <th class="col-sm-1">Deuda</th>
                    </tr>
                </thead>
                <tbody>
                </tbody>
            </table>
            <div id="LoadingPrepaid">
                <div align="center"><img src="~/Images/loading2.gif" width="25" height="25" /> Cargando .... </div>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/bundles/Content/Lib/BloqueoF12")
@Scripts.Render("~/bundles/Dashboard/PostpaidDataBilling/DebtDetail")