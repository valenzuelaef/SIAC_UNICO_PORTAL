@using System.Web.Optimization
@model Claro.SIACU.Web.WebApplication.Areas.Dashboard.Models.PostpaidDataService.DecoModel
    <div id="modal-content">
        <div id="PostpaidConsultaEquipmentsList">

     

            
                <div class="table-responsive">
                    <table id="tbDatosCliente" class="table table-condensed ">
                        <tr>
                            <td class="col-md-1">
                                <label class="control-label">Customer Id</label>
                            </td>
                            <td class="col-md-9">

                                <span id="lblPost_EquipmentsCustomerId"></span>


                            </td>
                            <td class="col-md-1"></td>
                        </tr>
                        <tr>
                            <td>
                                <label class="control-label">Cuenta</label>
                            </td>
                            <td> <span id="lblPost_EquipmentsCuenta"></span></td>
                            <td></td>
                        </tr>
                        <tr>
                            <td>
                                <label class="control-label">Cliente</label>
                            </td>
                            <td>
                                <span id="lblPost_EquipmentsCliente"></span>

                            </td>
                            <td></td>
                        </tr>
                        <tr>
                            <td>
                                <label class="control-label">Contacto</label>
                            </td>
                            <td><span id="lblPost_EquipmentsContacto"></span> </td>
                            <td></td>
                        </tr>

                    </table>

                    <table class="table  table-striped table-hover table-responsive claro-modal-tabla" id="tbEquipmentsPost">
                        <thead class="claro-bg-primary">
                            <tr>

                                <th class="col-sm-1">Cod. Material</th>
                                <th class="col-sm-1">Cod. SAP</th>
                                <th class="col-sm-1">Num. Serie</th>
                                <th class="col-sm-1">Mac Ad.</th>
                                <th class="col-sm-1">Desc. Material</th>
                                <th class="col-sm-1">Tipo Equipo</th>
                                <th class="col-sm-1">Código / Id Producto</th>
                                <th class="col-sm-1">Modelo</th>
                                <th class="col-sm-1">Conv. Type</th>
                                <th class="col-sm-1">Serv/Paque. Principal</th>
                                <th class="col-sm-1">Head End</th>
                                <th class="col-sm-1">Perfil</th>
                                <th class="col-sm-1">Número</th>
                            </tr>
                        </thead>
                        <tbody>
                            @{if (Model.lstDecoModel != null)
                            {

                                foreach (var item in Model.lstDecoModel)
                                {
                                    <tr>

                                        <td>@item.MaterialCode </td>
                                        <td>@item.SapCode</td>
                                        <td>@item.SerieNumber</td>
                                        <td>@item.AdressMac</td>
                                        <td>@item.MaterialDescripcion</td>
                                        <td>@item.EquipmentType</td>
                                        <td>@item.ProductId</td>
                                        <td>@item.Model</td>
                                        <td>@item.ConvertType</td>
                                        <td>@item.PricipalService</td>
                                        <td>@item.Headend</td>
                                        <td>@item.EphomeexChange</td>
                                        <td>@item.Number</td>


                                    </tr>
                                }
                            }

                            }



                        </tbody>
                    </table>
                </div>
          
        </div>
    </div>
@Scripts.Render("~/bundles/Content/Lib/BloqueoF12")
@Scripts.Render("~/bundles/Dashboard/PostpaidDataService/Equipments")