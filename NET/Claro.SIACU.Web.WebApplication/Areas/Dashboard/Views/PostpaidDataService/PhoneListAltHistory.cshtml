@using System.Web.Optimization
@model Claro.SIACU.Web.WebApplication.Areas.Dashboard.Models.Postpaid.DataLineServiceModel
<div id="modal-content">

    <div id="PostpaidPhoneListAltHistory">



        <div class="row">

            <div class="col-md-4">
                <input type="text" id="lblPost_PhoneListHistoryContractId" class="form-control input-sm" />

            </div>

            <div class="col-md-5">
                <button type="button" class="btn claro-btn-info btn-sm" data-toggle="modal" id="btnPost_TelephoneAltHistorySearch">Buscar</button>
                <button type="button" class="btn claro-btn-info btn-sm" data-toggle="modal" id="btnPost_TelephoneAltHistoryClear">Limpiar</button>
            </div>



        </div>



        <div id="divPostListAltHistorySearch">

            <table id="tbPostListAltHistory" class="table  table-striped table-hover table-responsive claro-modal-tabla">
                <thead class="claro-bg-primary">
                    <tr>
                        <th class="col-sm-2">Tel&#233;fono</th>
                        <th class="col-sm-2">Fecha Activación</th>
                        <th class="col-sm-2">Fecha Desactivación</th>


                    </tr>
                </thead>
                <tbody>
                    @{if (Model.lstServices != null)
                    {

                        foreach (var item in Model.lstServices)
                        {

                            <tr>
                                <td>@item.MSISDN</td>
                                <td>@item.ActivationDate</td>
                                <td>@item.DeactivationDate</td>
                            </tr>

                        }
                    }}


                </tbody>
            </table>

        </div>

    </div>
</div>
@Scripts.Render("~/bundles/Content/Lib/BloqueoF12")
@Scripts.Render("~/bundles/Dashboard/PostpaidDataService/PostpaidPhoneListAltHistory")