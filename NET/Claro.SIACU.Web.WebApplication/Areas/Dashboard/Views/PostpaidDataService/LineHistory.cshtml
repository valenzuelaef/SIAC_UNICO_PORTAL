@using System.Web.Optimization
@model Claro.SIACU.Web.WebApplication.Areas.Dashboard.Models.Postpaid.DataLineServiceModel
@{
    Layout = null;
}
<div id="modal-content">
    <div id="PospaidLineHistory">
        <table id="tbLineHistoryPost" class="table  table-striped table-hover table-responsive claro-modal-tabla">
            <thead class="claro-bg-primary">
                <tr>
                    <th class="col-sm-2">Estado</th>
                    <th class="col-sm-2">Motivo</th>
                    <th class="col-sm-2">Fecha</th>
                    <th class="col-sm-2">Programador</th>
                </tr>
            </thead>
            <tbody>
                @{if (Model.lstServices != null)
                {
                    foreach (var item in Model.lstServices)
                    {
                        <tr>
                            <td>@item.StateLine</td>
                            <td>@item.Reason</td>
                            <td>@item.Introduced</td>
                            <td>@item.IntroducedBy</td>
                        </tr>
                    }
                }}
            </tbody>
        </table>
    </div>
</div>
@Scripts.Render("~/bundles/Content/Lib/BloqueoF12")
@Scripts.Render("~/bundles/Dashboard/PostpaidDataService/LineHistory")


